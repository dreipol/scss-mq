{
  "name": "@dreipol/scss-mq",
  "version": "0.0.6",
  "description": "Dreipol scss media queries helper function",
  "main": "_index.scss",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "vendor",
    "lib",
    "_index.scss"
  ],
  "scripts": {
    "lint": "stylelint _index.scss lib/*.scss",
    "clean": "rm -rf vendor",
    "build": "npm run prebuild && rsync -rm `pwd`/node_modules/* --include=\"*.scss\" --include=\"*/\" --exclude=\"*\" vendor",
    "prebuild": "npm run clean && mkdir vendor",
    "prepublish": "npm run test",
    "test": "npm run build && npm run lint && sass test.scss > test.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreipol/scss-mq.git"
  },
  "keywords": [
    "sass",
    "scss",
    "media-queries",
    "media",
    "mq",
    "helpers",
    "css"
  ],
  "author": {
    "name": "dreipol GmbH",
    "email": "hello@dreipol.ch",
    "url": "http://dreipol.ch"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dreipol/scss-mq/issues"
  },
  "homepage": "https://github.com/dreipol/scss-mq#readme",
  "dependencies": {
    "@dreipol/scss-utils": "0.0.0",
    "include-media-or": "^1.0.1",
    "sass-to-js": "^1.3.1"
  },
  "devDependencies": {
    "@dreipol/stylelint-config": "^2.0.2",
    "stylelint": "^8.0.0"
  }
}
